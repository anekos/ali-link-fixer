{
  "applications": {
    "gecko": {
      "id": "ali-link-fixer@snca.net"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.aliexpress.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "description": "Fix Aliexpress annoying links",
  "manifest_version": 2,
  "name": "Ali Link Fixer",
  "version": "0.1"
}
